(define (passed-class? grade)
  (if (or (equal? "A" grade)
          (equal? "B" grade)
          (equal? "C" grade)
          (equal? "D" grade)) #t #f))



;criar lista de cursos que passou
(define (make-course-list a)
  (let loop ([l a] [r '()])
    (cond [(null? l) r]
          [(passed-class? (hash-ref (car l) 'conceito ""))
           (loop (cdr l) (append r (list (hash-ref (car l) 'codigo ""))))]
          [else (loop (cdr l) r)])))



